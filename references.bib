	keywords = {frequency distribution, law of large numbers, sample size, sampling distribution},
	pages = {33--51},
	file = {Full Text PDF:C\:\\Users\\thackmann\\Zotero\\storage\\4Z4RKPIJ\\Sedlmeier and Gigerenzer - 1997 - Intuitions about sample size the empirical law of large numbers.pdf:application/pdf;Snapshot:C\:\\Users\\thackmann\\Zotero\\storage\\J3PRYRP4\\(SICI)1099-0771(199703)10133AID-BDM2443.0.html:text/html},
