	keywords = {Confidence interval, Effective sample size, Kaplan–Meier curve},
	pages = {679--687},
	file = {Full Text PDF:C\:\\Users\\thackmann\\Zotero\\storage\\DQDBU88G\\Dorey and Korn - 1987 - Effective sample sizes for confidence intervals fo.pdf:application/pdf;Snapshot:C\:\\Users\\thackmann\\Zotero\\storage\\CEUT36RX\\sim.html:text/html},
